<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="style.css">
</head>

<body>
    <div class="container">
        <div class="screen">
            <div class="screen__content">
                <form class="login">
                    <div class="login__field">
                        <table>
                            <tr>
                                <td>Name</td>
                                <td>Age</td>
                                <td>Description</td>
                            </tr>
                            <% obj.forEach(function(justOne) { %>
                                <tr>
                                    <td>
                                        <%= justOne.name %>
                                    </td>
                                    <td>
                                        <%= justOne.age %>
                                    </td>
                                    <td>
                                        <%= justOne.description %>
                                    </td>
                                    <td><a class="update" href="update/<%= justOne._id %>">Update</a></td>
                                    <td><a class="delete" href="delete/<%= justOne._id %>">Delete</a></td>
                                </tr>
                                <% }); %>
                        </table>
                        <form action="/addName" method="post">
                            <input id="PrName" type="text" required name="name" placeholder="Name"><br><br>
                            <input id="PrAge" type="number" name="age" placeholder="Age"><br><br>
                            <input id="PrEmail" type="text" name="email" placeholder="Email"><br><br>
                            <input id="PrPas" type="text" name="password" placeholder="Password"><br><br>
                            <input id="PrDes" type="text" name="description" placeholder="Description"><br><br>
                            <input id="id" type="hidden" name="id"><br><br>
                            <input type="submit" onclick="getVal()">
                        </form>
                    </div>

                    <script src="script.js"></script>
            </div>
        </div>
    </div>
    </div>

</body>

</html>