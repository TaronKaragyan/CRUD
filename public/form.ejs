<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="style.css">
</head>
<body>
    <ul>
        <% obj.forEach(function(justOne) { %>
        <li>
        Id - <strong><%= justOne.theaterId %></strong> <br>
        City - <strong><%= justOne.location.address.city %></strong> /
        State - <strong><%= justOne.location.address.state %></strong> /
        Zip code - <strong><%= justOne.location.address.zipcode %></strong> /
        Street - <strong><%= justOne.location.address.street1 %></strong> <br> <br>
        </li>
        <% }); %>
    </ul>
    <form action="/submit" method="post">
        <input type="text" required name="name" placeholder="Please type your name"><br><br>
        <input type="text" name="email" placeholder="Please type your email"><br><br>
        <input type="text" name = "password" placeholder="Please type in your password"><br><br>
        <input type="submit">
    </form>

</body>
</html>